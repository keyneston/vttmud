{
  "name": "vttmud-server",
  "version": "0.0.1",
  "private": true,
  "description": "Backend API for VTTMUD",
  "main": "serve.js",
  "scripts": {
    "serve": "node src/serve.js",
    "build": "webpack build",
    "watch": "npm-watch serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "import_items": "node src/scripts/import_items.ts",
    "migrate": "node-pg-migrate"
  },
  "watch": {
    "serve": {
      "patterns": [
        "src",
        "../public"
      ],
      "quiet": false,
      "extensions": "js,jsx,ts,tsx,css,html"
    }
  },
  "author": "",
  "license": "",
  "dependencies": {
    "express": "^4.18.2",
    "minimist": "^1.2.8",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "npm-watch": "^0.11.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  }
}
